    "#module1\n",
    "import face_recognition\n",
    "import cv2\n",
    "import sys\n",
    "import pandas as pd\n",
    "from PIL import Image,ImageDraw,ImageFont\n",
    "import pygame\n",
    "import os\n",
    "import datetime\n",
    "ef=pd.read_csv('./top10rich.csv')\n",
    "empno=ef[\"Employee\"].tolist()\n",
    "firstname=ef[\"name\"].tolist()  # python list \n",
    "photolocation=ef[\"filename\"].tolist()\n",
    "audiolocation=ef[\"audio\"].tolist()\n",
    "n=len(empno)\n",
    "emp=[]\n",
    "emp_encode=[]\n",
    "audio=[]\n",
    "for i in range(n):\n",
    "    emp.append(face_recognition.load_image_file(photolocation[i]))\n",
    "    emp_encode.append(face_recognition.face_encodings(emp[i])[0])\n",
    "    \n",
    "#MODULE2\n",
    "# photo capture using camera\n",
    "camera =cv2.VideoCapture(0)\n",
    "for i in range(5):\n",
    "    return_value ,image=camera.read()           # return_value return the size\n",
    "    print(return_value,image.shape)\n",
    "    path ='C:\\\\Users\\\\suman\\\\Face_rec\\\\photos'\n",
    "    cv2.imwrite(os.path.join(path ,'Employee'+str(i)+\".png\"), image)\n",
    "    \n",
    "    #cv2.imwrite('photo'+str(i)+\".png\",image)    # imwrite is method we store the image in HDD\n",
    "del(camera)\n",
    "#uk=face_recognition.load_image_file(\"C:\\\\Users\\\\suman\\\\Face_rec\\\\photos\\\\Employee3.png\")\n",
    "\n",
    "\n",
    "grp_photo=face_recognition.load_image_file(\"C:\\\\Users\\\\suman\\\\Face_rec\\\\photos\\\\Employee3.png\")\n",
    "\n",
    "#MODULE_3\n",
    "def identify_employee(Employee):\n",
    "    try:\n",
    "        uk_encode = face_recognition.face_encodings(Employee)[0]\n",
    "    except :\n",
    "        print('error')\n",
    "        sys.exit(1)\n",
    "\n",
    "    found =face_recognition.compare_faces(emp_encode,uk_encode,tolerance=0.5)\n",
    "    print(found)\n",
    "    \n",
    "    index = -1\n",
    "    for i in range(0,n):\n",
    "        if found[i]:\n",
    "            index =i\n",
    "    return(index)\n",
    "\n",
    "\n",
    "emp_index= identify_employee(grp_photo)\n",
    "print(emp_index)\n",
    "\n",
    "\n",
    "\n",
    "#MODULE_4 attendance recod in a data file attendance.txt\n",
    "if(emp_index!=-1):\n",
    "    x=str(datetime.datetime.now())\n",
    "    eno=str(empno[emp_index])\n",
    "    f=firstname[emp_index]\n",
    "    ar=\"\\n\"+eno+\" \"+f+\" \"+x\n",
    "    f=open(\"C:\\\\Users\\\\suman\\\\Face_rec\\\\attendance\\\\attendance.txt\",'a')\n",
    "    f.write(ar)\n",
    "    f.close()\n",
    "    print(ar)\n",
    "    \n",
    "\n",
    "#Module5\n",
    "\n",
    "pil_uk=Image.fromarray(grp_photo)\n",
    "fl=face_recognition.face_locations(grp_photo) # f1 list of tuples with 4 elements \n",
    "draw=ImageDraw.Draw(pil_uk)\n",
    "fnt=ImageFont.truetype(\"arial.ttf\", 40, encoding=\"unic\")\n",
    "face_count=len(fl)\n",
    "for i in range(face_count):\n",
    "    top,right,bottom,left= fl[i]\n",
    "    draw.rectangle((left,top,right,bottom),\n",
    "                  outline=(255,0,0),width=3)\n",
    "\n",
    "if emp_index==-1:\n",
    "    name=\"Face Not Recognized\"\n",
    "else:\n",
    "    name= firstname[emp_index]   \n",
    "x=100\n",
    "y=grp_photo.shape[0]-100\n",
    "draw.text((x,y),name, font=fnt, fill=(255,0,0))\n",
    "pil_uk.show()\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "#Module6\n",
    "audioloc=audiolocation[emp_index]\n",
    "pygame.mixer.init()\n",
    "if emp_index==-1:\n",
    "    pygame.mixer.music.load('C:\\\\Users\\\\suman\\\\Face_rec\\\\audio\\\\failure.mp3')\n",
    "    pygame.mixer.music.play()\n",
    "else:\n",
    "    pygame.mixer.music.load(audioloc)\n",
    "    pygame.mixer.music.play()\n",
    "    pygame.mixer.music.queue('C:\\\\Users\\\\suman\\\\Face_rec\\\\audio\\\\welcome.mp3')\n",
    "    pygame.mixer.music.play()\n"
   ]
